package students;


public class Students {
String name;
String city;
int age;
void Printinfo(){
    System.out.println("Name:"+name);
        System.out.println("City:"+city);
            System.out.println("Age:"+age);


}
    public static void main(String[] args) {
Students  s1 =new Students();
Students  s2 =new Students();
s1.name="Rabail";
s1.city="Islamabad";
s1.age=20;
s2.name="Anum";
s2.city="Rawalpindi";
s2.age=19;
s1.Printinfo();
s2.Printinfo();

    }
}
